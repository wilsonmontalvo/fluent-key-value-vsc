{
    "name": "vsc-jsonsnippets",
    "displayName": "Fluent Key-Value",
    "description": "Makes writing JSON fluent, with a simple set of snippets.",
    "icon": "images/json_logo.png",
    "publisher": "wmontalvo",
    "license": "SEE LICENSE IN LICENSE.md",
    "version": "0.9.0",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "fluent",
        "key-value",
        "json",
        "snippet",
        "emmet"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wilsonmontalvo/vsc-jsonsnippets.git"
    },
    "activationEvents": [
        "onCommand:fluent-key-value.create"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "fluent-key-value.create",
                "title": "Fluent Key-Value: Create JSON (Beta)"
            }
        ],
        "snippets": [
            {
                "language": "json",
                "path": "./snippets/json.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/all-languages.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/all-languages.json"
            },
            {
                "language": "csharp",
                "path": "./snippets/csharp.json"
            },
            {
                "language": "razor",
                "path": "./snippets/csharp.json"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}
